{
  "name": "yano-js",
  "version": "0.0.29",
  "description": "Micro library and util functions",
  "author": "Scott Murphy <stmhawaii@gmail.com>",
  "main": "lib/index.js",
  "module": "lib/index.es.js",
  "types": "lib/index.d.ts",
  "directories": {
    "lib": "lib"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uxder/yano-js.git"
  },
  "files": [
    "lib"
  ],
  "ava": {
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register",
      "./_ava/_setup-browser-env.js"
    ],
    "files": [
      "src/**/*.spec.ts"
    ]
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "_ava",
      "**/*.d.ts",
      "docs/",
      "examples/",
      "coverage/",
      "lib/",
      "webpack/",
      "src/playground"
    ],
    "reporter": [
      "html",
      "text"
    ],
    "all": true
  },
  "devDependencies": {
    "@babel/core": "7.4.0",
    "@babel/plugin-proposal-class-properties": "7.4.0",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/polyfill": "7.4.0",
    "@babel/preset-env": "7.4.2",
    "@types/dat-gui": "0.6.3",
    "ava": "1.4.1",
    "babel-loader": "8.0.5",
    "babel-plugin-transform-es2015-modules-commonjs": "6.26.2",
    "browser-env": "3.2.6",
    "clean-webpack-plugin": "2.0.1",
    "concurrently": "4.1.0",
    "copy-webpack-plugin": "5.0.2",
    "css-loader": "2.1.1",
    "dat.gui": "0.7.6",
    "eslint": "5.16.0",
    "eslint-loader": "2.1.2",
    "express": "^4.17.1",
    "file-loader": "3.0.1",
    "html-webpack-plugin": "4.0.0-beta.5",
    "mini-css-extract-plugin": "0.5.0",
    "node-sass": "^4.11.0",
    "npm-run-all": "4.1.5",
    "nyc": "14.1.1",
    "sass-loader": "^7.1.0",
    "style-loader": "0.23.1",
    "pixi.js": "4.8.1",
    "ts-node": "^8.1.0",
    "tslint": "^5.16.0",
    "tslint-eslint-rules": "5.4.0",
    "typedoc": "0.14.2",
    "typedoc-plugin-external-module-map": "^1.0.0",
    "typedoc-plugin-no-inherit": "1.1.8",
    "typescript": "3.4.1",
    "webpack": "^4.29.6",
    "webpack-cli": "3.3.0",
    "webpack-dev-middleware": "^3.7.0",
    "webpack-dev-server": "3.2.1",
    "webpack-merge": "4.2.1"
  },
  "scripts": {
    "start": "webpack-dev-server --open --config webpack/webpack.config.dev.js",
    "server": "node server",
    "prepare": "npm run build",
    "build": "rm -rf lib && mkdir lib && tsc",
    "build-watch": "tsc --watch",
    "doc": "typedoc",
    "publish": "npm run doc && npm run coverage && npm run publish-doc && npm run publish-coverage",
    "publish-doc": "surge --domain yano-js.surge.sh --project $PWD/docs/",
    "publish-coverage": "surge --domain yano-js-coverage.surge.sh --project $PWD/coverage/",
    "doc-server": "simplehttpserver ./docs",
    "dev": "concurrently \"tsc --watch\" \"npm run start\" \"./node_modules/.bin/ava --watch\"",
    "dev-no-test": "concurrently \"tsc --watch\" \"npm run start\"",
    "test": "tsc && ava --watch",
    "ava": "ava",
    "coverage": "nyc ava",
    "lint": "tslint -c tslint.json \"src/**/*.ts\"",
    "lint-fix": "tslint --fix -c tslint.json \"src/**/*.ts\""
  },
  "dependencies": {}
}
